pcm_slave.out {
    pcm "hw:Loopback,0,0"
    rate 192000
    channels 2
    period_size 384
    buffer_size 1152
}

pcm_slave.in {
    pcm "hw:Loopback,0,1"
    rate 192000
    channels 1
    period_size 384
    buffer_size 1152
}

pcm.OEAudio {
    type asym
    playback.pcm "out"
    capture.pcm "in"
}

pcm.!default {
  type plug
  slave OEAudio
}

